{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "db-lib": {
      "type": "library",
      "root": "libs/db-lib",
      "entryFile": "index",
      "sourceRoot": "libs/db-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/db-lib/tsconfig.lib.json"
      }
    },
    "orders": {
      "type": "library",
      "root": "libs/orders",
      "entryFile": "index",
      "sourceRoot": "libs/orders/src",
      "compilerOptions": {
        "tsConfigPath": "libs/orders/tsconfig.lib.json"
      }
    },
    "orders-lib": {
      "type": "library",
      "root": "libs/orders-lib",
      "entryFile": "index",
      "sourceRoot": "libs/orders-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/orders-lib/tsconfig.lib.json"
      }
    }
  }
}